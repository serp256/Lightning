+partialclean::
+	cd tools; $(MAKE) clean
+
+alldepend::
+	cd tools; $(MAKE) depend
+
+# OCamldoc
+
+ocamldoc: ocamlc ocamlyacc ocamllex otherlibraries
+	cd ocamldoc && $(MAKE) all
+
+ocamldoc.opt: ocamlc.opt ocamlyacc ocamllex
+	cd ocamldoc && $(MAKE) opt.opt
